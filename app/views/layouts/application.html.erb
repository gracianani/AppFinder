        	</div><!--/#filter-summary -->
        </div><!--/#filter -->
        
        <div id="main" class="container">
	        <div id="primary" class="waterfall clearfix">
	        		<%= yield %>
	        </div><!--/#primary -->
	        
	        <div id="secondary">
	        </div><!--/#secondary -->



