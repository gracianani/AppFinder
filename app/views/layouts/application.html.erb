	        	</div><!-- /tag -->    
	        	<div id="filter-device" class="filter-btn">
	        			<i class="icon-device"></i>
	        	</div><!-- /device -->

        		<div id="filter-keyword" class="filter-btn form-search">
					    <input type="text" class="search-query">
					    <i class="icon-search"></i>
        		</div><!-- /keyword -->  
        		<div id="filter-advanced" class="filter-btn">
        			Advanced Filter
        		</div>  <!-- /advanced -->        		
        	</div><!--/#filter-box -->
