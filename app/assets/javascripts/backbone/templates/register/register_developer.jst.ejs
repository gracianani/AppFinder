<fieldset>
<legend>Your Company</legend>
<div class="control-group">
  <label  for="inputCompanyName">Name of the Company</label>
  <input type="text" id="inputCompanyName" class="input-xlarge" placeholder="">
</div>
<div class="control-group">
  <label  for="selectCountry">Country where you're located</label>
  <select id="selectCountry" class="input-xlarge">
  	<option value="US">US</option>
  </select>
</div>
<div class="control-group">
  <label for="inputAddress1">Address1:</label>
  <input type="text" name="" id="inputAddress1" />
  <label for="inputAddress2">Address2:</label>
  <input type="text" name="" id="inputAddress2" />
  <label for="inputCity">City:</label><input type="text" name="" id="inputCity" />
  <label for="selectState">State:</label>
  <select id="selectState">
  	<options value="AL">Alabama</options>
  	<options value="AK">Alaska</options>
  	<options value="AZ">Arizona</options>
  	<options value="AR">Arkansas</options>
  	<options value="CA">California</options>
  </select>
  <label for="inputZipcode">Zipcode:</label><input type="text" name="" id="inputZipcode" />
</div>
<div class="control-group">
  	<label for="textareaCompanyDescription">Enter a short description of the company</label>
  	<textarea id="textareaCompanyDescription" rows="10" class="input-xlarge"> </textarea> 
</div>
<div class="control-group">
  	<label for="inputCompanyUrl">Company's URL, including http://</label>
    <input type="text" id="inputCompanyUrl" class="input-xlarge"></input>
</div>
<div class="control-group">  
    <label for="inputCompanyFacebook">Company's Facebook profile URL (optional)</label>
    <input type="text" id="inputCompanyFacebook" class="input-xlarge"></input>
</div>
<div class="control-group">  
    <label for="inputCompanyTwitter">Company's Twitter profile URL (optional)</label>
    <input type="text" id="inputCompanyTwitter" class="input-xlarge"></input>
</div>
<div class="control-group">
	<label for="inputContactEmail">Company's contact email:</label>
	<input type="email" id="inputContactEmail" class="input-xlarge"></input>
</div>
<div class="contro-group">
	<label for="inputContactPhone">Tel:</label>
	<input type="tel" id="inputContactPhone" class="input-xlarge"></input>
</div>
</fieldset>
<fieldset>

<div class="control-group">
	<label for="inputAppFee">Your typical fee per App</label>
	<div class="input-prepend input-append">
	  <span class="add-on">$</span>
	  <input class="span2" id="inputAppFee" type="text">
	  <span class="add-on">K</span>
	</div>
</div>
</fieldset>
<fieldset>
	<legend>Your Apps</legend>
	<div class="accordion" id="accordionApps">
    </div>
</fieldset>
<div class="form-actions">
	<button type="submit" id="saveCompany" class="btn btn-info pull-right btn-large" >Save your profile</button>
</div>